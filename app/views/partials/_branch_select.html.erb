<%= form_for @branches, url: '.', method: 'get' do |f| %>
  <%= f.label :branch, 'Branch name' %>

  <%= f.select(:branch_select) do %>
    <% @branches.branch_select.each do |c| -%>
      <%= content_tag(:option, c.first, value: c.last, selected: (c.first == @branches.current_branch)) %>
    <% end %>
  <% end %>
  <%= f.submit 'show branch' %>
<% end %>
